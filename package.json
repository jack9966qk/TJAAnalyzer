{
  "name": "tja-analyzer",
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "fetch-ese": "node fetch-ese-assets.js",
    "build": "tsc && node prepare-dist.js",
    "test": "npm run build && node --loader ts-node/esm test-parser.ts && node --loader ts-node/esm test-exporter.ts && playwright test",
    "start": "npm run build && http-server app_package -c-1",
    "build:exe": "npm run build && neu build",
    "start:exe": "npm run build && neu run",
    "deploy": "npm run build && gh-pages -d app_package"
  },
  "devDependencies": {
    "@neutralinojs/neu": "^11.6.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.1",
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@neutralinojs/lib": "^6.4.0"
  }
}
